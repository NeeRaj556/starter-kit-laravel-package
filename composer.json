{
  "name": "neeraj/catalyst-starterkit-fastapi",
  "description": "Laravel Starter Kit with Authentication and CRUD Repository",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Niraj Bajagain",
      "email": "neeraj.bj556@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "laravel/framework": "^10.0",
    "tymon/jwt-auth": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "neeraj\\catalyst-starterKit-fastApi\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "neeraj\\catalyst-starterKit-fastApi\\Providers\\RepositoryServiceProvider",
        "neeraj\\catalyst-starterKit-fastApi\\Providers\\StarterKitProvider"
      ]
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php artisan jwt:secret --force",
      "@php artisan storage:link"
    ]
  },
}
